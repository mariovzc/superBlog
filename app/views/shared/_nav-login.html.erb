<%= form_tag sessions_path ,class: '', method: :post do %>
  </br></br>
  <div class="container-fluid">
    <div class="form-group">
      <%= label_tag :email, "Correo electronico" %>    
      <%= email_field_tag :email, nil, class: 'form-control', required: true, placeholder: 'Correo electronico' %>  
    </div>
    <div class="form-group">
        <%= label_tag :email, "Contraseña" %>    
        <%= password_field_tag :password, nil, class: 'form-control', required: true, placeholder: 'Contraseña' %>  
    </div>
    <%= submit_tag 'Log In', class: 'btn btn-primary' %>          
  </div>
<% end %>